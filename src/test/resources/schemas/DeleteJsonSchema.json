    },
    "members": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "id": {
              "type": "integer"
            },
            "name": {
              "type": "string"
            },
            "gender": {
              "type": "string"
            }
          },
          "required": [
